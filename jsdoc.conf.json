{
    "tags": {
        "allowUnknownTags": false,
        "dictionaries": ["jsdoc", "closure"]
    },
    "source": {
        "include": ["src"],
        "includePattern": ".ts$",
        "excludePattern": "(node_modules|docs)"
    },
    "plugins": ["better-docs/typescript"],
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    }
}